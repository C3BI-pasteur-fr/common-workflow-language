{
  "adapters": [
    {
      "argValue": {
        "@type": "Transform", 
        "language": "javascript", 
        "value": "$job.allocatedResources.cpu"
      }, 
      "position": 1, 
      "prefix": "-t"
    }
  ], 
  "baseCmd": [
    "bwa", 
    "mem"
  ], 
  "inputs": [
    {
      "depth": 0, 
      "id": "#reference", 
      "schema": {
        "adapter": {
          "position": 2
        }, 
        "type": "File"
      }
    }, 
    {
      "depth": 1, 
      "id": "#reads", 
      "schema": {
        "adaper": {
          "position": 3
        }, 
        "items": "File", 
        "type": "array"
      }, 
      "streamable": true
    }, 
    {
      "depth": 0, 
      "id": "#minimum_seed_length", 
      "schema": {
        "adapter": {
          "position": 1, 
          "prefix": "-m"
        }, 
        "type": "int"
      }
    }, 
    {
      "depth": 1, 
      "id": "#min_std_max_min", 
      "schema": {
        "adapter": {
          "itemSeparator": ",", 
